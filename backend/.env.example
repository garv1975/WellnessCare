# Flask Configuration
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-key-here

# Google OAuth (get from https://console.cloud.google.com)
GOOGLE_CLIENT_ID=paste-your-google-client-id-here
GOOGLE_CLIENT_SECRET=paste-your-google-client-secret-here

# Agora Video (get from https://console.agora.io)
AGORA_APP_ID=paste-your-agora-app-id-here
AGORA_APP_CERTIFICATE=paste-your-agora-app-certificate-here

# Database (automatically set by Render in production)
DATABASE_URL=postgresql://user:password@host:5432/database